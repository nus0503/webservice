      body {
        background-color: #fff;
        color: #313a46;
        font-size: 100%;
        line-height: 1.5em;
        font-family: sans-serif;
      }
      .container {
        display: flex;
        justify-content: center;
        margin-top: 150px;
      }
      .button-div {
        display: flex;
        justify-content: center;
        align-items: center;
        margin: 50px 0px;
      }

      .button {
        width: 160px;
        height: 42px;
        padding: 7px 37px 8px;
        border-radius: 30px;
        background-color: #058be6;
        color: white;
        border: none;
        font-size: 1.0em;
      }

      .button:hover {
        background-color: rgb(0, 170, 141);
        cursor: pointer;
      }
      .text-div {
        margin-left: 90px;
      }
      h1 {
        font-size: 6.0em;
        color: rgb(0, 170, 141);
        margin: 0;
        line-height: 1.4em;
      }
      h3 {
        font-size: 1.5em;
        margin: 0;
        margin-top: 10px;
      }
      p {
        font-size: 1.0em;
        margin: 0;
        margin-top: 10px;
      }

      a { text-decoration:none; color: white; }
    </style>
